      "| child7 = [[Bahnaric languages|Bahnaric]]\n",
      "| child8 = ''[[Khmer language|Khmer]]''\n",
      "| child9 = [[Pearic languages|Pearic]]\n",
      "| child10 = [[Monic languages|Monic]]\n",
      "| child11 = [[Aslian languages|Aslian]]\n",
      "| child12 = [[Nicobarese languages|Nicobarese]]\n",
